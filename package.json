{
  "name": "a-javascript-and-typescript-documentation-generator-based-on-typescript-compiler",
  "files": [
    "lib",
    "bin"
  ],
  "bin": {
    "a-javascript-and-typescript-documentation-generator-based-on-typescript-compiler": "./bin/cli.js"
  },
  "version": "0.0.1-1",
